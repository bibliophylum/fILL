<script type="text/javascript">

$.ajaxSetup ({  
  cache: false  
});  

$('document').ready(function(){
    $.getJSON('/cgi-bin/rotations-allowed.cgi', 
        {lid: <TMPL_VAR name="lid"> },
        function(data){
            if (data.allowed == "1") {
                $("#menu_rotations").show();
            } else {
                $("#menu_rotations").hide();
            }
        })
        .success(function() {
        })
        .error(function() {
        })
        .complete(function() { 
            // hack for admin:
            if (<TMPL_VAR name="lid"> === 101) {
              $("#menu_rotations_admin").show();
            }
        });

    update_menu_counters( <TMPL_VAR name="lid"> );
});

</script>


