<TMPL_INCLUDE NAME="header.tmpl">
<TMPL_INCLUDE NAME="submenu_zerocirc.tmpl">

<div id="main">

<a href="?rm=browse_form">Get new list...</a>
<TMPL_IF NAME="hideclaimed">
    <a href="?rm=browse_results_form&hideclaimed=0&collection=<TMPL_VAR NAME=collection>&call_start=<TMPL_VAR NAME=call_start>&call_end=<TMPL_VAR NAME=call_end>">Show already claimed items</a>
<TMPL_ELSE>
    <a href="?rm=browse_results_form&hideclaimed=1&collection=<TMPL_VAR NAME=collection>&call_start=<TMPL_VAR NAME=call_start>&call_end=<TMPL_VAR NAME=call_end>">Hide already claimed items</a>
</TMPL_IF>
<input class="noPrint" type="button" value="Print this list" onclick="window.print()">

<div id="middlecontent">
  <table cellpadding=20 cellspacing=5 border=0>
    <tr>
      <th>id</th>
      <th><!-- Claim! --> </th>
      <th>Call #</th>
      <th>PubDate</th>
      <th>Author</th>
      <th width="40%">Title</th>
      <!-- <th>claimed by</th> -->
    </tr>

    <TMPL_IF NAME="hideclaimed">

      <TMPL_LOOP NAME="browselist">
	<TMPL_UNLESS NAME="claimed_by">
	  <tr>
 	    <td><p style="color: gray"><TMPL_VAR NAME="id"></p></td>
	    <!-- <td align=right><p><input type="checkbox" name=chk<TMPL_VAR NAME="id">></p></td> --><td><p /></td>
	    <td><p style="background: #FFFFF0"><TMPL_VAR NAME="callno"></p></td>
	    <td><p style="background: #FFFFF0"><TMPL_VAR NAME="pubdate"></p></td>
	    <td><p style="background: #FFFFF0"><TMPL_VAR NAME="author"></p></td>
	    <td><p style="background: #FFFFF0"><TMPL_VAR NAME="title"></p></td>
	    <!-- <td><p style="background: #FFFFF0"><TMPL_VAR NAME="claimed_by"></p></td> -->
	  </tr>
	</TMPL_UNLESS>
      </TMPL_LOOP>

    <TMPL_ELSE>

      <TMPL_LOOP NAME="browselist">
	<TMPL_IF NAME="claimed_by">
	  <tr>
   	    <td><p style="color: gray"><del><TMPL_VAR NAME="id"></del></p></td>
	    <td><p></p></td>
	    <td><p><del><TMPL_VAR NAME="callno"></del></p></td>
	    <td><p><del><TMPL_VAR NAME="pubdate"></del></p></td>
	    <td><p><del><TMPL_VAR NAME="author"></del></p></td>
	    <td><p><del><TMPL_VAR NAME="title"></del></p></td>
	    <!-- <td><p><TMPL_VAR NAME="claimed_by"></p></td> -->
	  </tr>
	  <TMPL_ELSE>
	    <tr>
 	      <td><p style="color: gray"><TMPL_VAR NAME="id"></p></td>
	      <!-- <td align=right><p><input type="checkbox" name=chk<TMPL_VAR NAME="id">></p></td> --><td><p /></td>
	      <td><p style="background: #FFFFF0"><TMPL_VAR NAME="callno"></p></td>
	      <td><p style="background: #FFFFF0"><TMPL_VAR NAME="pubdate"></p></td>
	      <td><p style="background: #FFFFF0"><TMPL_VAR NAME="author"></p></td>
	      <td><p style="background: #FFFFF0"><TMPL_VAR NAME="title"></p></td>
	      <!-- <td><p style="background: #FFFFF0"><TMPL_VAR NAME="claimed_by"></p></td> -->
	    </tr>
	</TMPL_IF>
      </TMPL_LOOP>
      
    </TMPL_IF>

  </table>
</div>


<div id="rightcontent">
</div>

</div>

<TMPL_INCLUDE NAME="footer.tmpl">
