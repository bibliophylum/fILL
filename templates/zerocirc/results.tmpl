	    <td></td>
	  </TMPL_IF>
	  <td><p style="background: #FFFFF0"><TMPL_VAR NAME="callno"></p></td>
	  <td><p style="background: #FFFFF0"><TMPL_VAR NAME="pubdate"></p></td>
	  <td><p style="background: #FFFFF0"><TMPL_VAR NAME="author"></p></td>
	  <td><p style="background: #FFFFF0"><TMPL_VAR NAME="title"></p></td>
	  <!-- <td><p style="background: #FFFFF0"><TMPL_VAR NAME="claimed_by"></p></td> -->
