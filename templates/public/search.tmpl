 function increaseFontSize() {
    $('body').find('div, h1, h2, h3, p, td').each(function () {
        $(this).css({
            'font-size': parseFloat($(this).css('font-size')) + 1
        });
    });
 }
 function decreaseFontSize() {
    $('body').find('div, h1, h2, h3, p, td').each(function () {
        $(this).css({
            'font-size': parseFloat($(this).css('font-size')) - 1
        });
    });
  } 

</script>

