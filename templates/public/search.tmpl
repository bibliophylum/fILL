<script type="text/javascript">
  $('document').ready(function(){

    $("#image").append("<img src=/img/fill-search.jpg>");

    $("#query").focus();

    $(".coverlink").click(function( event ) {
        event.preventDefault();
        var $title = $(this).attr("title");
        $("#query").val( $title );
        $("#search-submit-button").click();
    });

  });
</script>

